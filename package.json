{
  "name": "atom-isort",
  "main": "./lib/index",
  "version": "3.1.0",
  "description": "Organize python imports using isort",
  "repository": "https://github.com/lexicalunit/atom-isort",
  "license": "MIT",
  "engines": {
    "atom": ">=1.4.0 <2.0.0"
  },
  "package-deps": [
    "linter:2.0.0"
